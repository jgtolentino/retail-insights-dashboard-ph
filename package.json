{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "./dev-local.sh",
    "build": "vite build",
    "build:prod": "VITE_APP_ENV=production vite build --mode production",
    "build:dev": "vite build --mode development",
    "preview:prod": "vite preview",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy:check": "./scripts/deployment-check.sh",
    "predeploy": "npm run deploy:check",
    "deploy": "vercel --prod",
    "deploy:vercel": "npm run build:prod && vercel --prod",
    "deploy:safe": "npm run deploy:vercel && npm run qa:finalize",
    "qa:finalize": "node scripts/postdeploy-verify.js",
    "qa:screenshot": "node scripts/screenshot-validator.js",
    "rollback": "./scripts/auto-rollback.sh",
    "setup": "node scripts/setup-env.js",
    "test": "vitest --passWithNoTests",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:agents": "vitest tests/agents/",
    "test:integration": "vitest run tests/integration --passWithNoTests",
    "test:e2e": "playwright test",
    "test:console": "echo 'Playwright tests to be implemented'",
    "validate:sprint": "node -e \"import('./src/utils/pre-sprint-checks.js').then(m => m.runPreSprintChecks(1).then(m.displayValidationResults))\"",
    "validate:pre-deploy": "node scripts/pre-deployment-validation.js",
    "test:all": "npm run validate:pre-deploy",
    "test:unit": "vitest run tests/unit --passWithNoTests",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "verify:env": "node scripts/verify-env.js",
    "debug:env": "node scripts/debug-env.js",
    "validate:env": "node scripts/validate-env.js",
    "verify:supabase": "node scripts/verify-supabase-config.js",
    "fix:supabase-config": "echo 'Supabase client now uses environment variables only. Please ensure your .env file is configured.'",
    "sql": "node scripts/auto-execute-sql.js",
    "sql:file": "node scripts/auto-execute-sql.js",
    "sql:functions": "node scripts/auto-execute-sql.js scripts/implement-filter-sync-functions.sql",
    "sql:test": "node scripts/auto-execute-sql.js",
    "capture:visuals": "tsx scripts/capture-visuals.ts",
    "capture:visuals:watch": "nodemon --watch src --exec 'npm run capture:visuals'",
    "capture:current": "npm run capture:visuals",
    "docs:visual": "npm run capture:visuals && echo 'Visual documentation generated in ./visual-docs'",
    "docs:changelog": "tsx scripts/generate-visual-changelog.ts",
    "commit:task": "npm run capture:visuals && git add visual-docs/* && git commit",
    "setup:visual-hooks": "chmod +x scripts/setup-visual-hooks.sh && ./scripts/setup-visual-hooks.sh",
    "prepare": "husky",
    "qa:check-orphans": "ts-node scripts/qa/check_orphan_tests.ts",
    "qa:generate-scaffolds": "ts-node scripts/qa/generate_test_scaffold.ts",
    "qa:update-matrix": "ts-node scripts/qa/update_matrix.ts",
    "ai:diagnostics": "node scripts/ai-diagnostics.js",
    "ai:autofix": "node scripts/ai-autofix.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
